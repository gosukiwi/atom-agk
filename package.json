{
  "name": "atom-agk",
  "main": "./lib/main",
  "version": "0.9.1",
  "description": "AppGameKit support from Atom.",
  "keywords": [
    "agk",
    "appgamekit"
  ],
  "activationCommands": {
    "atom-workspace": "atom-agk:generate-project"
  },
  "activationHooks": [
    "source.agk:root-scope-used"
  ],
  "repository": "https://github.com/gosukiwi/atom-agk",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "etch": "^0.14.0"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "provide"
      }
    }
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
