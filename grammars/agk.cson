scopeName: 'source.agk' # <scope> should be a short, unique indicator for the language ("js", "php", "c", etc.)
name: 'AppGameKit' # The title that will show up in grammar selection and on your status bar.
fileTypes: [ # An array of file extensions.
  'agc'
]
foldingStartMarker: '(?i)\\b(function|type|for|do|while|repeat|if|select|case)\\b' # A regular expression that checks for the start of a foldable area (such as `{`).
foldingStopMarker: '(?i)\\b(endfunction|endtype|endfor|loop|endwhile|until|endif|endselect|endcase)\\b' # A regular expression that checks for the end of a foldable area (such as `}`). If both folding markers are matched in the same line, there will be no foldable area.
patterns: [
  {
    begin: '(?i)remstart'
    end: '(?i)remend'
    name: 'comment.block.agk'
  }
  {
    begin: '(?i)/\\*'
    end: '(?i)\\*/'
    name: 'comment.block.agk'
  }
  {
    match: '(?i)(//|rem\s|`)[^\\n]*$' # A regular expression that matches some content. Optional.
    name: 'comment.line.double-slash.agk' # A period-delimited list of classes that will be applied to the content identified by `match`. Optional.
  }
  {
    match: '(?i)\s*#(include|insert|constant|option_explicit|company_name|import_plugin)\\b' # A regular expression that matches some content. Optional.
    name: 'constant.other.agk'
  }
  {
    match: '(?i)\\b(string|integer|float)\\b'
    name: 'constant.language.agk'
  }
  {
    match: '"([^"]|\\")*?"'
    name: 'string.quoted.double.agk'
  }
  {
    match: '(?i)\\b(type)\\s+((?![0-9]+)[a-zA-Z0-9_]+)\\b'
    captures:
      1:
        name: 'keyword.other.agk'
      2:
        name: 'entity.name.type.agk'
  }
  {
    match: '(?i)\\b(as)\\s+(integer|float|string)\\b'
    captures:
      1:
        name: 'keyword.other.agk'
      2:
        name: 'constant.language.agk'
  }
  {
    match: '(?i)\\b(as)\\s+((?![0-9]+)[a-z0-9_]+)\\b'
    captures:
      1:
        name: 'keyword.other.agk'
      2:
        name: 'entity.name.type.agk'
  }
  {
    match: '(?i)\\b(endtype|function|endfunction)\\b' # A regular expression that matches some content. Optional.
    name: 'keyword.other.agk'
  }
